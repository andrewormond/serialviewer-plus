<reactiveui:ReactiveWindow  x:Class="SerialViewer_Plus.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        x:TypeArguments="viewmodels:TerminalViewModel"
        xmlns:local="clr-namespace:SerialViewer_Plus"
        xmlns:reactiveui="http://reactiveui.net"
        xmlns:viewmodels="clr-namespace:SerialViewer_Plus.ViewModels"
        xmlns:lvc="clr-namespace:LiveChartsCore.SkiaSharpView.WPF;assembly=LiveChartsCore.SkiaSharpView.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="10"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <lvc:CartesianChart x:Name="chart" ></lvc:CartesianChart>
        <lvc:CartesianChart x:Name="fftView" Grid.Row="2"></lvc:CartesianChart>
    </Grid>
</reactiveui:ReactiveWindow>
